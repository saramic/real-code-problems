#!/usr/bin/env bash

set -e

RED='\033[0;31m'
GREEN='\033[0;32m'
YELLOW='\033[0;33m'
NC='\033[0m'

_usage() {
  echo
  echo -e "Hi $(whoami)!"
  echo
  echo -e "${GREEN}Workflow${NC}"
  echo
  echo -e "${YELLOW}makze list_c${NC} list Challenges"
  echo -e "${YELLOW}makze zip_c${NC} ZIP each challenge"
}
_usage
